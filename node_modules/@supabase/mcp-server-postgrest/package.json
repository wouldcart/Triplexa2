{
  "name": "@supabase/mcp-server-postgrest",
  "version": "0.1.0",
  "description": "MCP server for PostgREST",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "tsup --clean",
    "prepublishOnly": "npm run build",
    "test": "vitest"
  },
  "files": [
    "dist/**/*"
  ],
  "bin": {
    "mcp-server-postgrest": "./dist/stdio.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.cjs"
    }
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "@supabase/sql-to-rest": "^0.1.6",
    "zod": "^3.24.1",
    "zod-to-json-schema": "^3.24.1"
  },
  "devDependencies": {
    "@supabase/mcp-utils": "*",
    "@total-typescript/tsconfig": "^1.0.4",
    "@types/node": "^22.8.6",
    "prettier": "^3.3.3",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vitest": "^2.1.4"
  }
}
